{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "esnext",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "target": "es2020",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["jest"]
  },
  "include": ["./src"]
}
